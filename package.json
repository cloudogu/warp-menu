{
  "name": "warp-menu",
  "version": "1.7.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudogu/warp-menu"
  },
  "devDependencies": {
    "postcss-cli": "^11.0.0",
    "postcss-prefix-selector": "^1.16.1",
    "prettier": "^3.3.3",
    "vite": "^5.3.5",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-plugin-zip-pack": "^1.2.4"
  },
  "scripts": {
    "dev": "yarn serve",
    "build": "rm -rf target && vite build",
    "serve": "vite preview --outDir .",
    "plaintw": "tailwindcss -i node_modules/@cloudogu/ces-theme-tailwind/ces-theme-tailwind-config.css src/warp.css -o ./src/generated.css",
    "tw": "yarn plaintw && postcss ./src/generated.css -o ./src/generated.css",
    "watch": "yarn plaintw --watch"
  },
  "dependencies": {
    "@cloudogu/ces-theme-tailwind": "^0.8.0",
    "@mertasan/tailwindcss-variables": "^2.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "tailwindcss-aria-attributes": "^2.0.1",
    "tailwindcss-scoped-groups": "^2.0.0"
  }
}
